<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework-message-queue:etc/consumer.xsd">
    <consumer name="PaypalSubscriptionRelease"
              queue="paypal_subscription_release"
              connection="amqp"
              consumerInstance="Magento\Framework\MessageQueue\Consumer"
              handler="PayPal\Subscription\Model\ReleaseConsumer::execute" />
    <consumer name="PaypalSubscriptionReleaseDb"
              queue="paypal_subscription_release_db"
              connection="db"
              handler="PayPal\Subscription\Model\ReleaseConsumer::execute"/>
</config>
